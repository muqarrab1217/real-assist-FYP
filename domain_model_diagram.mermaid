classDiagram
    direction TB

    %% User Management Module - Light Blue
    class User {
        +String id
        +String email
        +String name
        +String role
        +Date createdAt
        +String status
        +String avatar
        +String phone
        +String address
    }

    class Client {
        +String clientId
        +String userId
        +String cnic
        +String phone
        +String address
        +Number totalInvestment
        +String status
        +Date registrationDate
        +String preferredContact
        +String emergencyContact
    }

    class Admin {
        +String adminId
        +String userId
        +String permissions
        +String department
        +String accessLevel
        +Date lastLogin
        +String role
        +Boolean isActive
    }

    %% Lead Management Module - Light Red
    class Lead {
        +String leadId
        +String name
        +String email
        +String phone
        +String source
        +String status
        +Number score
        +String assignedTo
        +Date createdAt
        +Date lastContact
        +String notes
        +String priority
        +Number budget
    }

    class LeadSource {
        +String sourceId
        +String name
        +String type
        +Number conversionRate
        +String description
        +Date createdDate
        +Boolean isActive
        +String category
    }

    class LeadAssignment {
        +String assignmentId
        +String leadId
        +String assignedTo
        +Date assignedDate
        +String priority
        +String status
        +String notes
        +Date dueDate
    }

    %% Project Management Module - Light Green
    class Project {
        +String projectId
        +String name
        +String location
        +String developer
        +String status
        +Date startDate
        +Date completionDate
        +Number totalUnits
        +Number soldUnits
        +String description
        +String[] amenities
        +Number progress
    }

    class Property {
        +String propertyId
        +String projectId
        +String unitNumber
        +String type
        +Number bedrooms
        +Number area
        +String view
        +Number price
        +String status
        +String amenities
        +String floor
        +String building
    }

    class ProjectUpdate {
        +String updateId
        +String projectId
        +String title
        +String description
        +String milestone
        +Number progress
        +String[] images
        +Date createdAt
        +String category
        +String priority
        +String status
    }

    %% Investment Management Module - Light Cyan
    class Investment {
        +String investmentId
        +String clientId
        +String propertyId
        +Number amount
        +Number downPayment
        +Number remainingAmount
        +String status
        +Date investmentDate
        +Number expectedROI
        +String investmentType
        +String currency
        +Date maturityDate
    }

    class Portfolio {
        +String portfolioId
        +String clientId
        +Number totalValue
        +Number totalReturns
        +String riskLevel
        +Date lastUpdated
        +Number numberOfInvestments
        +String performance
        +String strategy
    }

    %% Payment Management Module - Light Yellow
    class Payment {
        +String paymentId
        +String investmentId
        +Number amount
        +String type
        +String method
        +String status
        +Date dueDate
        +Date paidDate
        +String transactionId
        +String receiptNumber
        +String bankReference
        +String currency
    }

    class PaymentPlan {
        +String planId
        +String propertyId
        +Number downPaymentPercentage
        +Number totalInstallments
        +Number monthlyAmount
        +Number quarterlyAmount
        +String status
        +Date startDate
        +Date endDate
        +String planType
        +Number interestRate
    }

    class Installment {
        +String installmentId
        +String paymentPlanId
        +Number installmentNumber
        +Number amount
        +Date dueDate
        +String status
        +Number lateFee
        +String paymentMethod
        +Date paidDate
        +String transactionId
    }

    %% Feedback Management Module - Light Purple
    class Feedback {
        +String feedbackId
        +String clientId
        +String type
        +String subject
        +String message
        +String status
        +String priority
        +String assignedTo
        +Date createdAt
        +Date resolvedAt
        +String resolution
        +String category
    }

    class SupportTicket {
        +String ticketId
        +String feedbackId
        +String category
        +String severity
        +String[] attachments
        +String resolution
        +Date resolvedAt
        +String resolvedBy
        +String status
        +String priority
    }

    %% Analytics Module - Light Orange
    class Analytics {
        +String analyticsId
        +Number totalLeads
        +Number activeClients
        +Number totalRevenue
        +Number monthlyRevenue
        +Number conversionRate
        +Number averageDealSize
        +Date calculatedAt
        +String period
        +Number[] trends
        +String reportType
    }

    class Report {
        +String reportId
        +String type
        +String title
        +String[] data
        +String format
        +Date generatedAt
        +String generatedBy
        +String status
        +String[] filters
        +String description
    }

    class DashboardStats {
        +String statId
        +String title
        +String value
        +Number change
        +String changeType
        +String icon
        +Date lastUpdated
        +String category
        +String unit
    }

    %% Value Objects - Light Gray
    class Address {
        +String street
        +String city
        +String province
        +String postalCode
        +String country
        +String landmark
        +String coordinates
    }

    class ContactInfo {
        +String email
        +String phone
        +String alternatePhone
        +String emergencyContact
        +String preferredContact
        +String whatsapp
        +String skype
    }

    class FinancialDetails {
        +Number totalAmount
        +Number paidAmount
        +Number remainingAmount
        +Number interestRate
        +String currency
        +Date lastUpdated
        +String paymentMethod
        +String bankAccount
    }

    %% Relationships
    User --> Client
    User --> Admin
    User --> Lead
    Client --> Investment
    Client --> Payment
    Client --> Feedback
    Client --> Portfolio
    Project --> Property
    Project --> ProjectUpdate
    Project --> PaymentPlan
    Property --> Investment
    Property --> PaymentPlan
    Investment --> Payment
    Investment --> Portfolio
    PaymentPlan --> Installment
    Lead --> LeadSource
    Lead --> LeadAssignment
    Lead --> Client
    Feedback --> SupportTicket
    Analytics --> Report
    Analytics --> DashboardStats

    %% Value Object Relationships
    Client --> Address
    Client --> ContactInfo
    Investment --> FinancialDetails
    Payment --> FinancialDetails

    %% Styling for different modules
    classDef userModule fill:#e3f2fd,stroke:#1976d2,stroke-width:3px,color:#000
    classDef leadModule fill:#ffebee,stroke:#d32f2f,stroke-width:3px,color:#000
    classDef projectModule fill:#e8f5e8,stroke:#388e3c,stroke-width:3px,color:#000
    classDef investmentModule fill:#e0f7fa,stroke:#00acc1,stroke-width:3px,color:#000
    classDef paymentModule fill:#fff9c4,stroke:#f57c00,stroke-width:3px,color:#000
    classDef feedbackModule fill:#f3e5f5,stroke:#7b1fa2,stroke-width:3px,color:#000
    classDef analyticsModule fill:#fff3e0,stroke:#f57c00,stroke-width:3px,color:#000
    classDef valueObject fill:#f5f5f5,stroke:#424242,stroke-width:2px,color:#000

    %% Apply styles to classes
    User:::userModule
    Client:::userModule
    Admin:::userModule
    Lead:::leadModule
    LeadSource:::leadModule
    LeadAssignment:::leadModule
    Project:::projectModule
    Property:::projectModule
    ProjectUpdate:::projectModule
    Investment:::investmentModule
    Portfolio:::investmentModule
    Payment:::paymentModule
    PaymentPlan:::paymentModule
    Installment:::paymentModule
    Feedback:::feedbackModule
    SupportTicket:::feedbackModule
    Analytics:::analyticsModule
    Report:::analyticsModule
    DashboardStats:::analyticsModule
    Address:::valueObject
    ContactInfo:::valueObject
    FinancialDetails:::valueObject